{
  "version": 2,
  "builds": [
    {
      "src": "api/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/app.js",
      "methods":["GET","POST","PUT","DELETE","PATCH","OPTIONS"],
      "headers":{"Acess-Control-Allow-Origin":"*"}
    }
  ]
}
